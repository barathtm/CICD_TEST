//ADCDCBP JOB (TEST),CLASS=A,MSGCLASS=X,MSGLEVEL=(1,1),NOTIFY=&SYSUID
//********************************************************************
//*    RUN UNDER IBMUSER                                             *
//*    BIND TO ALLOW DYNAMIC SQL IN BATCH                            *
//********************************************************************
//BIND01   EXEC PGM=IKJEFT01,
//             COND=(4,LT),
//             REGION=4096K
//STEPLIB  DD  DISP=SHR,
//             DSN=DSNC10.SDSNLOAD
//DBRMLIB  DD  DISP=SHR,
//             DSN=DSNC10.DBCG.DBRMLIB.DATA
//SYSPRINT DD  SYSOUT=*
//SYSTSPRT DD  SYSOUT=*
//SYSUDUMP DD  SYSOUT=*
//SYSTSIN  DD  *
    DSN SYSTEM (DBCG)
    BIND  PLAN (DSNTIAUL) -
    VALIDATE  (BIND)     -
    MEMBER (DSNTIAUL)     -
    ISOLATION (CS) -
    RELEASE   (COMMIT) -
    LIBRARY   ('DSNC10.DBCG.DBRMLIB.DATA') -
    ACTION    (REP) -
    OWNER     (IBMUSER) -
    QUALIFIER (IBMUSER) -
    ENCODING(EBCDIC)
/*
//BIND02   EXEC PGM=IKJEFT01,
//             COND=(4,LT),
