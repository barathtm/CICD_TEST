//CSDUP   JOB 1,JOHN,MSGCLASS=X
//* COPY GROUP #ADD FROM NTRS CSD TO POP-UP CSD
//CPYCSD EXEC PGM=DFHCSDUP,REGION=0M
//STEPLIB   DD DSN=DFH550.CICS.SDFHLOAD,DISP=SHR
//SYSPRINT  DD SYSOUT=*
//* TARGET CSD
//DFHCSD    DD DSN=DFH550.CICS.DFHCSD,DISP=SHR
//* SOURCE CSD
//SRCCSD    DD DSN=DFH550.CICS.DFHCSD.NT.IBM1.TEST,DISP=SHR
//SYSIN  DD *
 COPY GROUP(#ASM) TO(#ASM) REPLACE FROMCSD(SRCCSD)
/*
//*   PUT GROUP #ASM IN A LIST
//ADDGRP EXEC PGM=DFHCSDUP,REGION=0M,COND=(4,LT)
//STEPLIB   DD DSN=DFH550.CICS.SDFHLOAD,DISP=SHR
//SYSPRINT  DD SYSOUT=*
//* TARGET CSD
//DFHCSD    DD DSN=DFH550.CICS.DFHCSD,DISP=SHR
//SYSIN  DD *
 ADD GROUP(#ASM) LIST(XYZLIST)
/*
//
