//EDS   DD DSN=ADCDC.N5.UNLOAD2,
//            DISP=OLD
//ISPCTL1  DD DISP=NEW,UNIT=VIO,SPACE=(TRK,(5,5,0)),
//            DCB=(LRECL=80,BLKSIZE=6160,RECFM=FB)
//SYSIN    DD *
   SELECT PARTITION_YEAR_DTE, PROD_CONTRACT_NUM, TRANS_SER_NR,
   SEGMENT_NR_OF, TRAN_DETAIL
   FROM NWM.TRANDETAIL
   WITH UR
   FETCH FIRST 8000 ROWS ONLY
/*
