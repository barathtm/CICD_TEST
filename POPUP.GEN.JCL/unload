//*
//ISPTLIB  DD DSN=&TBLIBS,DISP=(OLD,DELETE,DELETE)
//*
//ISPPROF  DD DSN=&BATPROF,DISP=(NEW,DELETE,DELETE),
//            DCB=(BLKSIZE=3120,LRECL=80,DSORG=PO,RECFM=FB),
//            SPACE=(TRK,(1,,1)),UNIT=VIO
//CTEDS  DD DSN=ADCDC.N5.CTRL,
